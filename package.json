{
  "name": "mongo-db",
  "version": "1.0.0",
  "description": "MongoDB Epress TS REST API",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf /dist && tsc",
    "start": "node dist/index.js",
    "start-dev": "cross-env NODE_ENV=development npx nodemon",
    "lint": "npx eslint",
    "testdb": "cross-env NODE_ENV=test npx ts-node ./src/index.ts",
    "test": "npx playwright test --reporter=list"
  },
  "repository": {
    "type": "git",
    "url": "(https://github.com/Missjessen/MDB_REST.git)"
  },
  "author": "",
  "license": "ISC",
  "nodemonConfig": {
    "verbose": false,
    "watch": [
      "src"
    ],
    "ext": "js,ts",
    "exec": "npx ts-node ./src/index.ts",
    "ignore": [
      "node_modules"
    ]
  },
  "dependencies": {
    "@faker-js/faker": "^9.5.1",
    "@types/mongodb": "^4.0.6",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.5.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^4.1.0",
    "express": "^4.21.2",
    "google-auth-library": "^9.15.1",
    "googleapis": "^146.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.13.1",
    "mongoose": "^8.9.6",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.9",
    "streamifier": "^0.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.10",
    "@types/streamifier": "^0.1.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "keywords": []
}
