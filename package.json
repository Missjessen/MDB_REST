{
  "name": "mongo-db",
  "version": "1.0.0",
  "description": "MongoDB Epress TS REST API",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf /dist && tsc",
    "start": "node dist/index.js",
    "start-dev": "cross-env NODE_ENV=development npx nodemon",
    "lint": "npx eslint",
    "testdb": "cross-env NODE_ENV=test npx ts-node ./src/index.ts",
    "test": "node --max-old-space-size=4096 node_modules/.bin/playwright test --reporter=list",
    "test:api": "jest --passWithNoTests",
    "test:api:watch": "jest --watch",
    "test:e2e": "playwright test --reporter=list"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Missjessen/MDB_REST.git"
  },
  "author": "",
  "license": "ISC",
  "nodemonConfig": {
    "verbose": false,
    "watch": [
      "src"
    ],
    "ext": "js,ts",
    "exec": "npx ts-node ./src/index.ts",
    "ignore": [
      "node_modules"
    ]
  },
  "dependencies": {
    "@faker-js/faker": "^9.5.1",
    "@mapbox/node-pre-gyp": "^2.0.0",
    "@types/mongodb": "^4.0.6",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.6.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "dotenv-flow": "^4.1.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "glob": "^9.3.5",
    "google-ads-api": "^19.0.2-rest-beta",
    "google-auth-library": "^9.15.1",
    "googleapis": "^146.0.0",
    "joi": "^17.13.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lru-cache": "^11.1.0",
    "mongodb": "^6.13.1",
    "mongoose": "^8.13.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "rimraf": "^4.4.1",
    "streamifier": "^0.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "resolutions": {
    "rimraf": "^4.0.0",
    "glob": "^9.0.0",
    "lodash": "^4.17.21",
    "lru-cache": "^9.0.0",
    "npmlog": "^6.0.0",
    "google-p12-pem": "^4.0.0",
    "q": "^2.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mongoose": "^5.11.96",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.10",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.12",
    "@types/streamifier": "^0.1.2",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "keywords": []
}
